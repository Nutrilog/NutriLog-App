<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:width="48dp"
    android:height="24dp"
    tools:ignore="NewApi">
    <item
        android:state_checked="true"
        android:state_enabled="false"
        android:drawable="@drawable/ic_track_id_disable" />

    <item
        android:state_checked="false"
        android:state_enabled="false"
        android:drawable="@drawable/ic_track_en_disable" />

    <item
        android:state_checked="true"
        android:state_enabled="true"
        android:drawable="@drawable/ic_track_id" />

    <item
        android:state_checked="false"
        android:state_enabled="true"
        android:drawable="@drawable/ic_track_en" />
</animated-selector>